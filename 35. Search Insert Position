class Solution {
public:
    int findVal(vector<int>& nums, int target, int start, int end){
        if(start==end) return start;
        int mid = start + (end - start) / 2;
        if(nums[mid] < target) return findVal(nums, target, mid+1, end);
        else return findVal(nums, target, start, mid);
    }

    int searchInsert(vector<int>& nums, int target) {
        return findVal(nums, target, 0, nums.size());
    }
};
